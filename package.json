{
  "name": "chimimoryo-server",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/index.ts",
    "build": "cross-env NODE_ENV=production tsc && tsc-alias",
    "start": "cross-env NODE_ENV=production node ./dist/index.js",
    "lint": "eslint --ext .ts .",
    "format": "prettier --write \"**/*.ts\"",
    "check:format": "prettier --check \"**/*.ts\"",
    "check:types": "tsc --pretty --noEmit",
    "check": "pnpm lint && pnpm check:format && pnpm check:types && pnpm build",
    "clean": "tsc --build --clean && rimraf --glob \"dist/\"",
    "prepare": "node ./scripts/prepare.js"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/multer": "^3.0.2",
    "@koa/router": "^12.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "formidable": "^3.5.1",
    "koa": "^2.14.2",
    "koa-bodyparser": "^4.4.1",
    "koa-logger": "^3.2.1",
    "koa-passport": "^6.0.0",
    "koa-session": "^6.4.0",
    "koa2-swagger-ui": "^5.9.0",
    "passport-discord": "^0.1.4",
    "passport-oauth2": "^1.7.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/koa": "^2.13.9",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa-logger": "^3.1.3",
    "@types/koa-mount": "^4.0.3",
    "@types/koa-passport": "^6.0.1",
    "@types/koa-session": "^6.4.2",
    "@types/koa__cors": "^4.0.1",
    "@types/koa__multer": "^2.0.5",
    "@types/koa__router": "^12.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.6.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
}
